{
  "openapi": "3.0.1",
  "info": {
    "title": "Swagger MilkTeaStore",
    "description": "This is a sample server MilkTeaStore server.",
    "version": "0.0.1"
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "https://swagger.io"
  },
  "tags": [
    {
      "name": "milkTea",
      "description": "Everything about your Milk Tea",
      "externalDocs": {
        "description": "Find out more",
        "url": "https://swagger.io"
      }
    }
  ],
  "paths": {
    "/milkTea": {
      "post": {
        "summary": "Add a new milkTea to the store",
        "operationId": "addMilkTea",
        "requestBody": {
          "description": "MilkTea object that needs to be added to the store",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MilkTea"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "405": {
            "description": "Invalid input",
            "content": {}
          }
        },
        "x-codegen-request-body-name": "body"
      },
      "get": {
        "summary": "Finds all MilkTeas",
        "description": "Multiple status values can be provided with comma separated strings",
        "operationId": "findAllMilkTeas",
        "parameters": [
          {
            "name": "names",
            "in": "query",
            "description": "Names to filter by",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "caffeinated",
            "in": "query",
            "description": "Filter by caffeinated",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MilkTea"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid status value",
            "content": {}
          }
        }
      }
    },
    "/milkTea/{milkTeaId}": {
      "get": {
        "summary": "Find milkTea by ID",
        "description": "Returns a single milkTea",
        "operationId": "getMilkTeaById",
        "parameters": [
          {
            "name": "milkTeaId",
            "in": "path",
            "description": "ID of milkTea to return",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MilkTea"
                }
              }
            }
          },
          "400": {
            "description": "Invalid ID supplied",
            "content": {}
          },
          "404": {
            "description": "MilkTea not found",
            "content": {}
          }
        }
      },
      "patch": {
        "summary": "Updates a milkTea in the store with form data",
        "operationId": "updateMilkTeaWithForm",
        "parameters": [
          {
            "name": "milkTeaId",
            "in": "path",
            "description": "ID of milkTea that needs to be updated",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "Updated name of the milkTea"
                  },
                  "price": {
                    "type": "string",
                    "description": "Updated price of the milkTea"
                  },
                  "caffeinated": {
                    "type": "boolean"
                  },
                  "toppings": {
                    "type": "array",
                    "description": "Update topping of the milkTea",
                    "items": {
                      "type": "string"
                    }
                  },
                  "ingredients": {
                    "type": "array",
                    "description": "Update the ingredients of the milkTea",
                    "items": {
                      "type": "string"
                    }
                  },
                  "size": {
                    "type": "string",
                    "description": "Update the size of the milkTea"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MilkTea"
                }
              }
            }
          },
          "405": {
            "description": "Invalid input",
            "content": {}
          }
        }
      },
      "delete": {
        "summary": "Deletes a milkTea",
        "operationId": "deleteMilkTea",
        "parameters": [
          {
            "name": "milkTeaId",
            "in": "path",
            "description": "MilkTea id to delete",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {}
          },
          "400": {
            "description": "Invalid ID supplied",
            "content": {}
          },
          "404": {
            "description": "MilkTea not found",
            "content": {}
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "MilkTea": {
        "required": [
          "name",
          "price"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "type": "string",
            "example": "Four Seasons Tea"
          },
          "ingredients": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "price": {
            "type": "string"
          },
          "caffeinated": {
            "type": "boolean"
          },
          "size": {
            "type": "string"
          },
          "toppings": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}